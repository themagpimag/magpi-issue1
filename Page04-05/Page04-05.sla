        <PAGEOBJECT OwnPage="1" PTYPE="4" XPOS="749.134488188976" YPOS="217.426456692913" WIDTH="325.984251968504" HEIGHT="599.37255859375" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="1" COLGAP="28.34645669" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESP="24" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="RightPageBodyText" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 325.984 0 325.984 0 325.984 0 325.984 0 325.984 599.373 325.984 599.373 325.984 599.373 325.984 599.373 0 599.373 0 599.373 0 599.373 0 599.373 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 325.984 0 325.984 0 325.984 0 325.984 0 325.984 599.373 325.984 599.373 325.984 599.373 325.984 599.373 0 599.373 0 599.373 0 599.373 0 599.373 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="3" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONT="Nimbus Roman No9 L Regular" FONTSIZE="16" FCOLOR="Red" FSHADE="100" CH="Home computer users were becoming less interested in writing their own programs."/>
            <para ALIGN="3" LINESP="24"/>
            <para ALIGN="3" LINESP="24"/>
            <ITEXT FONT="Nimbus Roman No9 L Regular" FONTSIZE="16" FCOLOR="Red" FSHADE="100" CH="Slick-looking, professionally made games that were coming out of software publishing companies were compelling."/>
            <para ALIGN="3" LINESP="24"/>
            <para ALIGN="3" LINESP="24"/>
            <ITEXT FONT="Nimbus Roman No9 L Regular" FONTSIZE="16" FCOLOR="Red" FSHADE="100" CH="Ocean Software for example, boasted an impressive library of hit game titles such as 'Daley Thompson's Decathlon', 'Chase HQ' and 'New Zealand Story'."/>
            <para ALIGN="3" LINESP="24"/>
            <para ALIGN="3" LINESP="24"/>
            <ITEXT FONT="Nimbus Roman No9 L Regular" FONTSIZE="16" FCOLOR="Red" FSHADE="100" CH="The trend of moving away from programming followed within the education sector. Early software packages like Wordstar, and Lotus 123 were difficult to use before the days of the graphical interface, but typing skills, word processing, spreadsheet and database training continued to be the focus. Programming was seen as a very specialist and niche area of study."/>
            <trail ALIGN="3" LINESP="24"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
