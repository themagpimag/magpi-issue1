            <ITEXT FONT="FreeSans Regular" FONTSIZE="16" CH="First Boot"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="When you boot for the first time, you will be able to configure your setup. Puppy allows you to store all your user data conveniently on a USB memory device so it is available next time you boot (regardless of which machine you are using)."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="16" CH="To Keep Your Data"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="After booting and configuring your setup,"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="1. Select &quot;Shutdown&quot; and &quot;Power-off computer&quot; from the bottom &quot;Menu&quot; button."/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="2. You will be prompted by the &quot;First shutdown: save session&quot; window. As recommended, choose to &quot;SAVE TO FILE&quot;."/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="3. We will select the defaults (carefully read the info there for other options)."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Bold Oblique" FONTSIZE="14" CH="First shutdown: Default Action"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="ask fido "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Select &quot;administrator&quot;"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="get ready "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Plug the USB memory"/>
            <para ALIGN="3"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="device in*"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="choose filesystem"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Select &quot;ext2&quot;"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="save-file name "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Leave blank"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="encryption "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Select NORMAL"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="size save-file "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Select 512MB**"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="sanity check "/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <tab FONT="FreeSans Regular" FONTSIZE="12"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="12" CH="Should match below!"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="4. After the 2fs file is written it may prompt &quot;copy .sfs from CD&quot;, this is optional (it will use 379MB extra on the USB memory device)."/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="5. The next time you boot with the USB memory device attached, it will load up your settings and programs from your last session.***"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
        <PAGEOBJECT OwnPage="0" PTYPE="4" XPOS="128.3470909395" YPOS="218.426452636719" WIDTH="513.070861816406" HEIGHT="600.944885253906" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="2" COLGAP="14.1732283464567" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESP="14" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" IFONT="FreeSans Regular" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="LeftPageBodyText" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="1" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 513.071 0 513.071 0 513.071 0 513.071 0 513.071 600.945 513.071 600.945 513.071 600.945 513.071 600.945 0 600.945 0 600.945 0 600.945 0 600.945 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 513.071 0 513.071 0 513.071 0 513.071 0 513.071 600.945 513.071 600.945 513.071 600.945 513.071 600.945 0 600.945 0 600.945 0 600.945 0 600.945 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="3" LAYER="3" BOOKMARK="0" NEXTITEM="2" BACKITEM="-1">
            <ITEXT FONT="FreeSans Bold" FONTSIZE="16" CH="What is RacyPy?"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="RacyPy, is a pre-packaged cd image which when &quot;booted&quot; (run from power up) will load a graphical desktop similar to a MS Windows or Apple Mac computer."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="The RacyPy image is setup as a “Live CD” which means it loads directly to memory and nothing is installed."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="RacyPy comes with a range of software ready to run, such as:"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="AbiWord - word processing"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="Gnumeric - spreadsheet"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="SeaMonkey - web-browsing"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="Python 2.7.2 and 3.1.4 - program-ming"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="mtPaint - graphic editing"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="MPlayer/Ogle/Pmusic -  video/"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="DVD/music playback"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="Geany/Vim - programming editors"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="14" CH="Plus many other useful utilities. If you decide to use RacyPy more than once you can also setup a USB Memory Device to store your user data on, this will also allow you to install any additional programs which will be loaded every time you reload it."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="16" CH="Get Ready!"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="14" CH="First, obtain the iso image file:"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="14" CH="    RacyPy4_1.iso (507Mb)"/>
            <para ALIGN="3" LINESP="20"/>
            <ITEXT FONT="FreeSans Oblique" CH="   "/>
            <ITEXT FONT="FreeSans Oblique" FONTSIZE="14" CH=" http://goo.gl/nGy8u"/>
            <para ALIGN="3" LINESP="20"/>
            <ITEXT FONTSIZE="14" CH="Depending on the method, you’ll need:"/>
            <para ALIGN="3" LINESP="20"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="14" CH=" Live CD"/>
            <ITEXT FONTSIZE="14" CH=" "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="14" CH=" Virtual"/>
            <para ALIGN="3"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="14" CH=" Machine"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
